{
  "$schema": "https://raw.githubusercontent.com/google-gemini/gemini-cli/main/schemas/settings.schema.json",
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "model": {
    "name": "gemini-3-flash-preview",
    "compressionThreshold": 0.7
  },
  "useWriteTodos": true,
  "tools": {
    "autoAccept": true,
    "shell": {
      "enableInteractiveShell": false,
      "showColor": false,
      "inactivityTimeout": 600
    },
    "core": [
      "read_file",
      "read_many_files",
      "write_file",
      "replace",
      "list_directory",
      "glob",
      "search_file_content",
      "google_web_search",
      "web_fetch",
      "save_memory",
      "write_todos",
      "run_shell_command",
      "codebase_investigator"
    ],
    "allowed": [
      "run_shell_command(ls)",
      "run_shell_command(dir)",
      "run_shell_command(pwd)",
      "run_shell_command(cd)",
      "run_shell_command(cat)",
      "run_shell_command(head)",
      "run_shell_command(tail)",
      "run_shell_command(less)",
      "run_shell_command(more)",
      "run_shell_command(wc)",
      "run_shell_command(sort)",
      "run_shell_command(grep)",
      "run_shell_command(find)",
      "run_shell_command(tree)",
      "run_shell_command(echo)",
      "run_shell_command(touch)",
      "run_shell_command(mkdir)",
      "run_shell_command(cp)",
      "run_shell_command(mv)",
      "run_shell_command(rm)",
      "run_shell_command(which)",
      "run_shell_command(where)",
      "run_shell_command(type)",
      "run_shell_command(command)",
      "run_shell_command(New-Item)",
      "run_shell_command(move)",
      "run_shell_command(Get-ChildItem)",
      "run_shell_command(Get-Content)",
      "run_shell_command(Set-Location)",
      "run_shell_command(git)",
      "run_shell_command(git*)",
      "run_shell_command(pnpm)",
      "run_shell_command(npm)",
      "run_shell_command(yarn)",
      "run_shell_command(bun)",
      "run_shell_command(npx)",
      "run_shell_command(node)",
      "run_shell_command(tsc)",
      "run_shell_command(esbuild)",
      "run_shell_command(vite)",
      "run_shell_command(uv)",
      "run_shell_command(pip)",
      "run_shell_command(python)",
      "run_shell_command(python3)",
      "run_shell_command(py)",
      "run_shell_command(ruff)",
      "run_shell_command(pyright)",
      "run_shell_command(mypy)",
      "run_shell_command(pytest)",
      "run_shell_command(uvicorn)",
      "run_shell_command(cargo)",
      "run_shell_command(rustc)",
      "run_shell_command(rustfmt)",
      "run_shell_command(clippy)",
      "run_shell_command(go)",
      "run_shell_command(dotnet)",
      "run_shell_command(java)",
      "run_shell_command(javac)",
      "run_shell_command(gradle)",
      "run_shell_command(mvn)",
      "run_shell_command(make)",
      "run_shell_command(cmake)",
      "run_shell_command(docker)",
      "run_shell_command(docker-compose)",
      "run_shell_command(podman)",
      "run_shell_command(kubectl)",
      "run_shell_command(ffmpeg)",
      "run_shell_command(ffprobe)",
      "run_shell_command(magick)",
      "run_shell_command(rembg)",
      "run_shell_command(repomix)",
      "run_shell_command(code)",
      "run_shell_command(cursor)",
      "run_shell_command(curl)",
      "run_shell_command(curl*)",
      "run_shell_command(wget)",
      "run_shell_command(jq)",
      "run_shell_command(yq)",
      "run_shell_command(tar)",
      "run_shell_command(unzip)",
      "run_shell_command(zip)",
      "run_shell_command(7z)",
      "run_shell_command(ssh)",
      "run_shell_command(scp)",
      "run_shell_command(rsync)",
      "run_shell_command(env)",
      "run_shell_command(printenv)",
      "run_shell_command(set)",
      "run_shell_command(man)",
      "run_shell_command(help)"
    ],
    "exclude": [
      "run_shell_command(git push)",
      "run_shell_command(git reset --hard)",
      "run_shell_command(rm -rf /)",
      "run_shell_command(sudo)",
      "run_shell_command(su)",
      "run_shell_command(chmod 777)",
      "run_shell_command(format)",
      "run_shell_command(mkfs)",
      "run_shell_command(dd)"
    ]
  }
}