/**
 * Unknown World - CRT   ( CSS SSOT)
 *
 * RULE-002 :   UI  -  /HUD  
 * Frontend Style Guide : CRT   
 *
 * @see vibe/ref/frontend-style-guide.md
 */

/* ============================================
   1. CRT   (CSS Variables)
   ============================================ */
:root {
  /*   - CRT    */
  --bg-color: #0d0d0d; /* :    */
  --text-color: #33ff00; /*  :   (CRT ) */
  --text-dim: #1a8000; /*  :   */
  --accent-color: #ff00ff; /* :  */
  --border-color: #33ff00; /* :   */
  --panel-bg: rgba(0, 20, 0, 0.3); /*  :   */
  --warning-color: #ffaa00; /* :  */
  --error-color: #ff3333; /* :  */

  /* CRT  */
  --crt-scanline: rgba(18, 16, 16, 0.1);
  --crt-flicker: 0.03;
  --glow-intensity: 5px;
  --crt-flicker-opacity-min: 0.97;
  --crt-flicker-opacity-max: 1;

  /*  */
  --font-main: 'NeoDunggeunmo', 'VT323', monospace;
  --font-micro-en:
    'Share Tech Mono', 'Consolas', monospace; /* U-037:     */
  --font-size-base: 16px;
  --font-size-lg: 1.2rem;
  --font-size-xl: 1.5rem;
  --font-size-2xl: 2rem;
  --line-height-base: 1.5;

  /*   (U-028:  ) */
  --font-size-xs: 0.75rem; /* 12px at 1.0 scale -    */
  --font-size-sm: 0.875rem; /* 14px at 1.0 scale -   */

  /* UI  (U-028:   ) */
  --ui-scale-factor: 1;

  /*  */
  --spacing-xs: 4px;
  --spacing-sm: 8px;
  --spacing-md: 16px;
  --spacing-lg: 24px;
  --spacing-xl: 32px;
  --border-radius: 4px;
  --max-width: 1400px;

  /*   */
  --header-height: 60px;
  --footer-height: 120px;
  --sidebar-width: 320px;
}

/* ============================================
   2.  
   ============================================ */
@font-face {
  font-family: 'NeoDunggeunmo';
  src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_2001@1.3/NeoDunggeunmo.woff')
    format('woff');
  font-weight: normal;
  font-display: swap;
}

/* ============================================
   3. Reset & Base
   ============================================ */
*,
*::before,
*::after {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}

html {
  /* U-028: UI   - font-size    */
  font-size: calc(var(--font-size-base) * var(--ui-scale-factor));
  line-height: var(--line-height-base);
}

body {
  font-family: var(--font-main);
  background-color: var(--bg-color);
  color: var(--text-color);
  min-height: 100vh;
  overflow: hidden;
  text-shadow: 0 0 var(--glow-intensity) var(--text-dim);
}

#root {
  min-height: 100vh;
  display: flex;
  flex-direction: column;
}

/* ============================================
   4. CRT  
   ============================================ */

/*   */
.crt-overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: linear-gradient(rgba(18, 16, 16, 0) 50%, rgba(0, 0, 0, 0.25) 50%);
  background-size: 100% 4px;
  pointer-events: none;
  z-index: 9999; /*   (/    ) */
  animation: flicker 0.15s infinite;
}

/* 
 * U-037:   
 * - Critical: (9999)     
 * - Ambient:    CRT  
 */

/* Critical   ( ) */
[data-ui-importance='critical'],
.ui-critical,
.phase-item,
.currency-value,
.action-card-cost,
.agent-error {
  position: relative;
  z-index: 10000;
}

/* Ambient   ( ) */
.game-title,
.panel-header,
.action-card-title {
  position: relative;
  z-index: 1; /* 9999   */
}

#root {
  min-height: 100vh;
}

/* ()  */
@keyframes flicker {
  0% {
    opacity: 0.97;
  }
  50% {
    opacity: 1;
  }
  100% {
    opacity: 0.98;
  }
}

/*   () */
.glitch {
  position: relative;
}

.glitch::before,
.glitch::after {
  content: attr(data-text);
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.glitch::before {
  left: 2px;
  text-shadow: -1px 0 #ff0000;
  clip-path: inset(0 0 0 0);
  animation: glitch-anim-1 5s infinite linear alternate-reverse;
}

.glitch::after {
  left: -2px;
  text-shadow: -1px 0 #0000ff;
  clip-path: inset(0 0 0 0);
  animation: glitch-anim-2 5s infinite linear alternate-reverse;
}

@keyframes glitch-anim-1 {
  0%,
  100% {
    clip-path: inset(0 0 95% 0);
  }
  20% {
    clip-path: inset(30% 0 50% 0);
  }
  40% {
    clip-path: inset(10% 0 70% 0);
  }
  60% {
    clip-path: inset(80% 0 5% 0);
  }
  80% {
    clip-path: inset(45% 0 35% 0);
  }
}

@keyframes glitch-anim-2 {
  0%,
  100% {
    clip-path: inset(95% 0 0 0);
  }
  20% {
    clip-path: inset(50% 0 30% 0);
  }
  40% {
    clip-path: inset(70% 0 10% 0);
  }
  60% {
    clip-path: inset(5% 0 80% 0);
  }
  80% {
