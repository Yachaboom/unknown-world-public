export const TurnOutputSchema = z
  .object({
    //   (Hard Gate)
    language: LanguageSchema.describe('  ( )'),
    narrative: z.string().describe('  ()'),
    economy: EconomyOutputSchema.describe('  (, )'),
    safety: SafetyOutputSchema.describe('  '),

    // UI  
